#!/usr/bin/env dash
fnMerge () {
    for name in "$@"; do
        git rev-parse --verify $name >/dev/null 2>/dev/null && git merge $name && exit
    done
}

name=$(basename $0)
if [ "$name" = "gitxMgMain" ]
then
    fnMerge "main" "master" "mainline" "default"
elif [ "$name" = "gitxMgDev" ]
then
    fnMerge "dev" "develop" "devel" "development"
fi